ault {\n  name: \"Ticket\",\n  props: {\n    ticket: Object,\n  },\n  data() {\n    return {\n      response: '',\n      error: ''\n    }\n  },\n  methods: {\n    time(d) {\n      return moment(d).format('D MMMM YYYY, h:mm:ss a');\n    },\n    async respond() {\n      try {\n        let response = await axios.put('/api/tickets/' + this.ticket._id, {\n          status: \"closed\",\n          response: this.response,\n        });\n        this.ticket = response.data.ticket;\n      } catch (error) {\n        this.error = error.response.message;\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\nh3 {\n  font-size: 12px;\n  font-weight: normal;\n  background-color: #ccc;\n  padding: 10px 20px;\n}\n\nlabel {\n  background-color: #000;\n  color: white;\n  padding: 5px;\n  border-radius: 30px;\n  font-size: 12px;\n  margin-right: 10px;\n}\n\ninput {\n  margin-right: 10px;\n}\n</style>"]}]}